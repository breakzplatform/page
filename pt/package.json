{
  "name": "joseli.to",
  "version": "0.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "tiddlywiki --listen",
    "build": "rm -rf .cache && tiddlywiki --build index",
    "minify": "html-minifier output/wiki.html -o output/index.html --remove-comments --collapse-whitespace --minify-css && rm output/wiki.html"
  },
  "dependencies": {
    "html-minifier": "^4.0.0",
    "tiddlywiki": "^5.1.23"
  }
}
