{
    "description": "Basic client-server edition",
    "plugins": [
        "tiddlywiki/tiddlyweb",
        "tiddlywiki/filesystem",
        "tiddlywiki/highlight",
		"tiddlywiki/codemirror"
    ],
    "themes": [
        "tiddlywiki/vanilla"
    ],
    "languages": [
        "pt-BR"
    ],
    "config": {
		"retain-original-tiddler-path": true
	},
    "build": {
        "index": [
            "--rendertiddler",
            "$:/plugins/tiddlywiki/tiddlyweb/save/offline",
            "wiki.html",
            "text/plain"
        ],
        "static": [
            "--rendertiddler",
            "$:/core/templates/static.template.html",
            "static.html",
            "text/plain",
            "--rendertiddler",
            "$:/core/templates/alltiddlers.template.html",
            "alltiddlers.html",
            "text/plain",
            "--rendertiddlers",
            "[!is[system]]",
            "$:/core/templates/static.tiddler.html",
            "static",
            "text/plain",
            "--rendertiddler",
            "$:/core/templates/static.template.css",
            "static/static.css",
            "text/plain"
        ],
        "feeds": [
			"--rendertiddler","$:/plugins/techlifeweb/xmlbuilder/static.template.html","static.html","text/plain",
			"--rendertiddlers","[!is[system]]","$:/plugins/techlifeweb/xmlbuilder/static.tiddler.html","static","text/plain",
			"--rendertiddler","$:/plugins/techlifeweb/xmlbuilder/sitemap","sitemap.xml","text/plain",
			"--rendertiddler","$:/rss/Notinhas","feed/notinhas.atom","text/plain",
			"--rendertiddler","$:/rss/Artigos","feed/artigos.atom","text/plain"]
    }
}